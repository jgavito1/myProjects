<ContentNavigationDrawer>:
    
    MDList:
        OneLineListItem:
            text: 'Dice Roller'
            on_press:
                root.nav_drawer.set_state('close')
                root.screen_manager.current = 'Dice Roller'
            
        
        OneLineListItem:
            text: 'Character Sheet'
            on_press:
                root.nav_drawer.set_state('close')
                root.screen_manager.current = 'Character Sheet'
        
        OneLineIconListItem:
            text: 'Close Window'
            on_press: 
                root.nav_drawer.set_state('close')
            IconLeftWidget:
                icon: 'keyboard-backspace'
                on_press:
                    root.nav_drawer.set_state('close')
